{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": true,
  "arrowParens": "always",
  "trailingComma": "none",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "objectWrap": "preserve",
  "bracketSameLine": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
